{
  "name": "andysnell/composer-replacements",
  "description": "A 'no-op' package with no dependencies used to cleanly replace other packages",
  "type": "library",
  "require": {
    "php": ">=7.3"
  },
  "replace": {
    "laminas/laminas-cache-storage-adapter-filesystem": "*",
    "laminas/laminas-cache-storage-adapter-memcache": "*",
    "laminas/laminas-cache-storage-adapter-memcached": "*",
    "laminas/laminas-cache-storage-adapter-memory": "*",
    "laminas/laminas-cache-storage-adapter-mongodb": "*",
    "laminas/laminas-cache-storage-adapter-session": "*",
    "laminas/laminas-cache-storage-adapter-wincache": "*",
    "laminas/laminas-cache-storage-adapter-xcache": "*",
    "laminas/laminas-cache-storage-adapter-zend-server": "*",
    "laminas/laminas-zendframework-bridge": "*",
    "symfony/polyfill-php73": "*"
  }
}
